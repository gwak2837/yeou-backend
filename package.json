  "main": "out/index.cjs",
  "repository": "https://github.com/rmfpdlxmtidl/coopang-backend.git",
  "scripts": {
    "dev": "NODE_ENV=development node esbuild.js & NODE_ENV=development nodemon -r dotenv/config out/index.cjs dotenv_config_path=.env.local.dev",
    "build": "NODE_ENV=production node esbuild.js",
    "start": "yarn build && NODE_ENV=production node -r dotenv/config out/index.cjs dotenv_config_path=.env.local",
    "clean": "rm -rf .yarn/cache .yarn/unplugged .yarn/install-state.gz out .pnp.*",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier . --write",
    "export": "tsc --project database/tsconfig.json && node database/dist/export.js",
    "import": "tsc --project database/tsconfig.json && node database/dist/import.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/swagger": "^8.2.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@google-cloud/storage": "^6.8.0",
    "@sinclair/typebox": "^0.25.11",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^2.4.0",
    "@pgtyped/cli": "^1.0.2",
    "@pgtyped/query": "^1.0.1",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "esbuild": "^0.15.18",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=18.2.0"
  },
  "private": true,
  "packageManager": "yarn@3.3.0",
  "resolutions": {
    "gaxios/node-fetch": "^3.2.10"
  },
  "type": "module"
}
