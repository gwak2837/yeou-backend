{
  "name": "coopang-backend",
  "version": "0.1.0",
  "description": "coopang-backend",
  "homepage": "https://github.com/rmfpdlxmtidl/coopang-backend#readme",
  "bugs": {
    "url": "https://github.com/rmfpdlxmtidl/coopang-backend/issues",
